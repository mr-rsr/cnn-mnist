{
    "version": 2,
    "builds": [
        {
            "src": "api/predict.py",
            "use": "@vercel/python"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "functions": {
        "api/predict.py": {
            "maxDuration": 30
        }
    }
}